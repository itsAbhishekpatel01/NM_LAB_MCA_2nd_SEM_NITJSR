#include <bits/stdc++.h>
using namespace std;
int main() {
    string s; cin>>s;
    for(int i=0;i<s.size();i++){
        char c=s[i];
        if(c=='A'||c=='E'||c=='I'||c=='O'||c=='U'||c=='a'||c=='e'||c=='i'||c=='o'||c=='u')
        s.erase(i,1);
    }
    cout<<s;
    return 0;
}

/*
Output:
Abhishek
bhshk
*/
